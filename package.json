{
  "name": "app-front-admin",
  "version": "1.0.0-snapshot",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/akveo/ngx-admin.git"
  },
  "bugs": {
    "url": "https://github.com/akveo/ngx-admin/issues"
  },
  "scripts": {
    "ng": "ng",
    "conventional-changelog": "conventional-changelog",
    "start:no:proxy": "ng serve --port 5400",
    "start": "ng serve --port 4600 --proxy-config proxy.conf.json",
    "start:prod": "ng serve --port 4002 --proxy-config proxy.conf.json",
    "build": "ng build",
    "build:prod": "npm run build -- --prod --aot --outputPath=target/prod",
    "build:qa": "ng build --configuration=qa --aot --outputPath=target/qa && tar -czf qa.tar.gz -C target/qa .",
    "build:demo": "npm run build -- -c=demo --aot --outputPath=target/demo",
    "build:qa:lm": "npm run build -c=qa --max_old_space_size=1024 --aot --outputPath=target/qa",
    "test": "ng test",
    "test:coverage": "rimraf coverage && npm run test -- --code-coverage",
    "lint": "ng lint",
    "lint:fix": "ng lint todo-front-v1 --fix",
    "lint:styles": "stylelint ./src/**/*.scss",
    "lint:ci": "npm run lint && npm run lint:styles",
    "eslint": "eslint '*/**/*.{js,ts,tsx}' --quiet",
    "eslint:fix": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "eslint:styles:fix": "eslint '*/**/*.{scss}' --quiet --fix",
    "eslint:ci": "npm run eslint:fix && npm run eslint:styles:fix",
    "format": "prettier --write \"./src/**/*.{ts,json,html}\"",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "ng e2e",
    "ng:version": "node ./node_modules/@angular/cli/bin/ng version",
    "docs": "compodoc -p src/tsconfig.app.json -d docs",
    "docs:serve": "compodoc -p src/tsconfig.app.json -d docs -s",
    "release:changelog": "npm run conventional-changelog -- -p angular -i CHANGELOG.md -s",
    "postinstall": "ngcc --properties es2015 es5 browser module main --first-only --create-ivy-entry-points --tsconfig \"./src/tsconfig.app.json\"",
    "prepare": "cd .. && husky install todo-front-v1/.husky",
    "deploy:s3:qa": "aws s3 sync target/qa/todo-front-v1/ s3://common-bo-qa.hacesbyallpa.link --profile allpa-freddycucho.adm",
    "deploy:s3:demo": "aws s3 sync target/demo/todo-front-v1/ s3://common-bo-demo.hacesbyallpa.link --profile allpa-freddycucho.adm",
    "deploy:qa": "npm run build:qa && npm run deploy:s3:qa",
    "deploy:demo": "npm run build:demo && npm run deploy:s3:demo"
  },
